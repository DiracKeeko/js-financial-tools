{
  "name": "js-financial-tools",
  "version": "2.0.1",
  "description": "javascript (& typescript) financial tools",
  "main": "dist/js-financial-tools.js",
  "scripts": {
    "dev": "rollup -c types.rollup.config.js && rollup -c",
    "dev:watch": "rollup -c types.rollup.config.js && rollup -c --watch",
    "min": "rollup -c types.rollup.config.js --environment NODE_ENV:production && rollup -c --environment NODE_ENV:production",
    "build": "npm run dev && npm run min",
    "types": "rollup -c types.rollup.config.js && rollup -c types.rollup.config.js --environment NODE_ENV:production",
    "test": "jest",
    "cov": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DiracKeeko/js-financial-tools.git"
  },
  "keywords": [
    "front-end",
    "js",
    "ts",
    "financial",
    "fund",
    "stock",
    "calc",
    "display",
    "Reg"
  ],
  "author": "DiracKeeko",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/DiracKeeko/js-financial-tools/issues"
  },
  "homepage": "https://github.com/DiracKeeko/js-financial-tools#readme",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/jest": "^29.1.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "core-js": "^2.6.12",
    "jest": "^29.1.2",
    "lodash": "^4.17.21",
    "mocha": "^9.2.1",
    "rollup": "^2.68.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  }
}
